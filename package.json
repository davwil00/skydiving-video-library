{
  "name": "skydiving-video-library-v2",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && tsx prisma/seed.ts",
    "migrate-dev": "prisma generate && prisma migrate dev",
    "migrate-deploy": "prisma generate && prisma migrate deploy",
    "typecheck": "tsc",
    "start": "remix-serve ./build/server/index.js"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "@remix-run/node": "^2.9.2",
    "@remix-run/react": "^2.9.2",
    "@remix-run/serve": "^2.10.2",
    "date-fns": "^3.6.0",
    "ffmetadata": "^1.7.0",
    "isbot": "^5.1.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.10.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.12.10",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "prisma": "^5.13.0",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.16.2",
    "typescript": "^5.1.6",
    "vite": "^5.3.3",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}

{
  "name": "skydiving-video-library-v2",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && tsx prisma/seed.ts",
    "migrate-dev": "prisma generate && prisma migrate dev",
    "migrate-deploy": "prisma generate && prisma migrate deploy",
    "typecheck": "react-router typegen && tsc",
    "test": "dotenv -e .env.test -- npx prisma migrate deploy && vitest --run",
    "start": "react-router-serve ./build/server/index.js",
    "generate": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.3.1",
    "@react-router/fs-routes": "^7.1.5",
    "@react-router/node": "^7.0.0",
    "@react-router/serve": "^7.0.0",
    "date-fns": "^4.1.0",
    "dotenv-cli": "^7.4.2",
    "ffmetadata": "^1.7.0",
    "isbot": "^5.1.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.0.0",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@react-router/dev": "^7.0.0",
    "@types/node": "^22.13.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.23",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.5.1",
    "prisma": "^5.21.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^5.4.10",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}

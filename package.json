{
  "name": "skydiving-video-library-v2",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && tsx prisma/seed.ts",
    "migrate-dev": "prisma generate && prisma migrate dev",
    "migrate-deploy": "prisma generate && prisma migrate deploy",
    "typecheck": "react-router typegen && tsc",
    "test": "dotenv -e .env.test -- npx prisma migrate deploy && vitest --run",
    "start": "react-router-serve ./build/server/index.js",
    "generate": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "@react-router/fs-routes": "^7.6.3",
    "@react-router/node": "^7.6.3",
    "@react-router/serve": "^7.6.3",
    "@tailwindcss/vite": "^4.1.11",
    "date-fns": "^4.1.0",
    "dotenv-cli": "^8.0.0",
    "ffmetadata": "^1.7.0",
    "isbot": "^5.1.28",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.3",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@react-router/dev": "^7.6.3",
    "@types/node": "^22.15.19",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.20",
    "daisyui": "^5.0.46",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.5.6",
    "prisma": "^6.11.1",
    "tailwindcss": "^4.1.7",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
